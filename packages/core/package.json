{
  "name": "@nearux/flow",
  "version": "0.0.0",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --config ../../eslint.config.mjs --max-warnings 0",
    "publish": "pnpm publish --access public --no-git-checks"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "tsup": "^8.0.0"
  },
  "dependencies": {
    "@nearux/shared": "workspace:*",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
